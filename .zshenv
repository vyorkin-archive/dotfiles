# Setting PATH for JRuby 1.7.4
# The orginal version is saved in .profile.jrubysave
PATH=$PATH:/usr/local/bin:$HOME/.rvm/bin:/usr/local/smlnj/bin:/Library/Frameworks/JRuby.framework/Versions/Current/bin:$HOME/bin:/Applications/Postgres93.app/Contents/MacOS/bin

[[ -s "$HOME/.rvm/scripts/rvm" ]] && . "$HOME/.rvm/scripts/rvm"

export LANG=en_US.UTF-8
export LC_ALL=$LANG
